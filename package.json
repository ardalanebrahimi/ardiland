{
  "name": "@ardiland/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:api": "nx serve api",
    "start:web": "nx serve web",
    "start": "nx run-many --target=serve --projects=api,web --parallel",
    "build:api": "nx build api",
    "build:web": "nx build web",
    "build": "nx run-many --target=build --all",
    "db:generate": "prisma generate --schema=api/src/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=api/src/database/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=api/src/database/prisma/schema.prisma",
    "db:seed": "npx tsx api/src/database/prisma/seed.ts",
    "db:studio": "prisma studio --schema=api/src/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=api/src/database/prisma/schema.prisma"
  },
  "private": true,
